{
    "env": {
        "es2021": true,
        "node": true,
        "browser": true,
        "mocha": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": "off",
        "no-await-in-loop": "off",
        "no-shadow": "off",
        "max-len": "off",
        "no-loop-func": "off"
    }
}